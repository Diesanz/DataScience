    "import numpy as np\n",
    "\n",
    "def bogosort(x):\n",
    "    while np.any(x[:-1] > x[1:]):\n",
    "        print(x[1:])\n",
    "        np.random.shuffle(x)\n",
    "         \n",
    "    return x"
   ]
